*{
	margin:0;
	padding:0;
}
.wrapper{
	margin:40px auto;
	margin-bottom:70px;
	border-radius:10px;
	padding-top:50px;
	padding-bottom:50px;
	box-shadow:0 0 40px 2px rgba(37,73,214,0.18);
}
.feature-box{
	padding:0 40px;
}
.feature-box h1{
	color:crimson;
}
.feature-box p{
	margin:50px auto;
	color:#555;
	font-size:18px;
	line-height:1.7;
}
.btn-one,.btn-two{
	text-decoration:none !important;
	padding:10px 30px;
	border-radius:30px;
	color:#fff;
	background:crimson;
	border:2px solid crimson;
	margin-right:30px;
}
.btn1{
	text-decoration:none !important;
	padding:1px 10px;
	border-radius:30px;
	color:#fff;
	background:crimson;
	border:2px solid crimson;
	margin-right:30px;
	font-size:15px;
	transition:0.2s ease;
}
.btn1:hover{
	background:#fff;
	color:crimson;
	border:1px solid crimson;
}
.arrow1{
	width:30px;
	margin:0 10px;
}
.feature-box .btn-two{
	background:none;
	color:crimson;
	
}
.feature-img{
	width:100%;
	padding:40px;
}
.
.feature-box .btn-two:hover{
	color:#fff;
	background:crimson;
	
}
#searchwrap {
  text-align: center;
  min-height: 10vh;
  display:flex;
  justify-content:center;
  align-items:center;
  width:100%;
  margin:10px 0;

}
#searchwrap .searchwrap2{
  width:450px;
  height:9.5vh;display:flex;
  justify-content:center;
  align-items:center;
  
  
}
#searchwrap .search-box {
  width: 350px;
  position: relative;
  display: flex;
	bottom: 0;
	left: 0;
	right: 0;
	margin: auto;
}

#searchwrap .search-input {
  width: 100%;
  font-family: 'Montserrat', sans-serif;
  font-size: 16px;
  padding: 10px 45px 10px 15px;
  background-color: #fff;
  color: #6c6c6c;
  border-radius: 30px;
  border:2px solid crimson;
  transition: all .4s;
}

#searchwrap .search-input:focus {
  border:2px solid red;
  outline:none;
  box-shadow: 0 1px 12px #b8c6db;
  -moz-box-shadow: 0 1px 12px #b8c6db;
  -webkit-box-shadow: 0 1px 12px #b8c6db;
}

#searchwrap .search-btn {
  background-color: transparent;
  font-size: 18px;
  padding: 6px 9px;
  margin-left:-45px;
  border:none;
  color: crimson;
  transition: all .4s;
  z-index: 10;
}

#searchwrap .search-btn:hover {
  transform: scale(1.2);
  cursor: pointer;
  color: black;
}

#searchwrap .search-btn:focus {
  outline:none;
  color:black;
}
